{
  "type": "commonjs",
  "name": "express-server",
  "version": "1.0.0",
  "description": "express server, nodejs",
  "main": "./dist/main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "lint": "eslint ./src/**",
